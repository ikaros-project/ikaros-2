<?xml version="1.0"?>

<class name="Text" description="tests a text parameter">

	<description type="text">
		Module that tests widget parameters of type text. The module has no other function and should probablly never be used.
	</description>

	<example description="A simple example">
		<module
			class="Text"
			name="Text"
            OUTPUT="ABC"
		/>
	</example>

	<parameter name="OUTPUT" type="Text" default="Alpha" description="parameter containing a string that is used as 'output' from the module through this parameter" />


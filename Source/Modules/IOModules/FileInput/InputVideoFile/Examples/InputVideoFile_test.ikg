<?xml version="1.0"?>

<group title="InputVideoFile Example">
	
		<module
			class = "InputVideoFile"
			name = "InputVideoFile"
            filename = "../../../../../../Examples/Media/ikaros.mp4"
			loop = "true"
		/>
		
	<view name="View">
		<image
		max_x = "1"
		title = "Intensity"
		width = "381"
		flipYCanvas = "no"
		x = "460"
		source = "InputVideoFile.INTENSITY"
		format = "gray"
		flipYAxis = "no"
		flipXCanvas = "no"
		h = "4"
		height = "321"
		max_y = "1"
		y = "60"
		flipXAxis = "no"
		scale = "both"
		class = "image"
		scales = "no"
		w = "4"
		/>
		
		<image
		max_x = "1"
		title = "Output"
		width = "381"
		flipYCanvas = "no"
		x = "60"
		source = "InputVideoFile.RED+GREEN+BLUE"
		format = "rgb"
		flipYAxis = "no"
		flipXCanvas = "no"
		h = "4"
		height = "321"
		max_y = "1"
		y = "60"
		flipXAxis = "no"
		scale = "both"
		class = "image"
		scales = "no"
		w = "4"
		/>
		
	</view>
    <module class = "Logger" name = "Logger" />
</group>
 

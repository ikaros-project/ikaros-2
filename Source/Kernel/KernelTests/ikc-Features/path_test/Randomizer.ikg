<?xml version="1.0"?>

<!--
 
    Randomizer.ikc

    This is an advanced example of some features of the IKC file format

    This file demonstrates inclusion of a module from
    different locations. This is an included module with outputsize set to 99.

    It includes the global module Randomizer using an absolute path starting
    in the project root (Source)
 
 -->

<group name="B">
    <output name="X" sourcemodule="RX" source="OUTPUT" />
    <module class="Randomizer" path="/Source/Modules/UtilityModules/Randomizer"  name="RX" _outputsize="11" />      <!-- should give an error if the path is incorrect -->
</group>


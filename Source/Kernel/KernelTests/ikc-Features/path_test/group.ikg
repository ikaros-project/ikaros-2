<?xml version="1.0"?>

<!--

    This is an advanced example of some features of the IKC file format
	
	This file demonstrates how modules can be imported from different locations
    using the path attribute on a module.
	
-->

<group name="A">
    <module class = "included_module" path="AAA" name = "R1" outputsize="1" />
    <module class = "included_module" path="BBB" name = "R2" outputsize="2" />
    <module class = "included_module" path="BBB/CCC" name = "R3" outputsize="3" />
    <module class = "included_module" name = "R4" outputsize="4" />
    <module class = "Randomizer" name = "R5" outputsize="5" />
    <module class = "Randomizer" name = "R6" outputsize="6" path="/Source/Modules/UtilityModules/Randomizer/" />
</group>

